{
  "compilerOptions": {
    "target": "ES2015",
    "module": "commonjs",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "allowJs": true,
    "jsx": "react",
    "outDir": "../js",
    "noImplicitAny": false
  },
  "include": [
    "*.tsx"
  ],
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "Recommended"
}